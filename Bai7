void plusMinus(vector<int> arr) {
    int plus=0;
    int pluss=0;
    int plusss=0;
    
    for (int i=0;i<arr.size();i++){
        
        if (arr[i]==0){
            plus++;
        }else if (arr[i]<0){
            pluss++;
        }else plusss++;
    }
    cout<<fixed<<setprecision(6)<<(float)plusss/arr.size()<<endl;
        cout<<fixed<<setprecision(6)<<(float)pluss/arr.size()<<endl;
    cout<<fixed<<setprecision(6)<<(float)plus/arr.size();

    
}

